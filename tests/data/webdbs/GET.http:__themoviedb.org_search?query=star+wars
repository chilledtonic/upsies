<main id="main" class="smaller  subtle show_search_true">
  

  <script>
  $(function() {
    initializeScroller(false, '#search_menu_scroller');
    active_nav_item = 'movie';

    History.Adapter.bind(window, 'statechange', function() {
      var State = History.getState();
      changeSearchTabs(State.data.media_type);
    });

    
      $('a.search_tab').on('click', function(e) {
        e.preventDefault();

        
          window.scrollTo(0, 0);
        

        
          History.pushState({ media_type: active_nav_item }, document.title, '/search?query=star wars');
        

        selected_media_type = $(this).prop('id')
        changeSearchTabs(selected_media_type);

        $('ul.with_counts li').removeClass('selected');
        $(this).parent().addClass('selected');

        
          History.pushState({ media_type: selected_media_type }, document.title, '/search/' + selected_media_type + '?query=star wars');
        

        
      });
    

    $("div.search_results div.results a.result").on('click', function(e) {
      var media_id = $(this).data('id');
      var media_type = $(this).data('media-type');
      var adult = $(this).data('media-adult');

      if (adult == false) {
        switch (media_type) {
          case 'movie':
          case 'tv':
          case 'person':
            $.ajax({
              url: '/search/track',
              type: 'PUT',
              data: {
                media_id: media_id,
                media_type: media_type
              }
            });
            break;
          default:
            break;
        }
      }
    });

  });
</script>

